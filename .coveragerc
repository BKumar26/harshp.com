[run]
branch = True
source = harshp

omit =
    /*/migration*
    /*/tests*
    /*/admin
    /*/suit
    /*/reveal_js
    /*/ckeditor
    /*/settings
    /*/__init__.py
    /*/__*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise NotImplementedError
    if __name__ == .__main__.:

ignore_errors = True